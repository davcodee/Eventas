<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deyvi.eventas">

    <application
        android:name=".SplashScreen.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/store"
        android:label="@string/app_name"
        android:roundIcon="@drawable/store"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login.LoginActivity"/>
        <activity
            android:name=".ContainerActivity"
            android:label="@string/title_activity_container"
            android:theme="@style/AppTheme" />
        <activity android:name=".SplashScreen.SplashActivity"
            android:theme="@style/splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>